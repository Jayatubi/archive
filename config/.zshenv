export ANDROID_SDK_ROOT=/Users/$USER/adt/sdk
export ANDROID_NDK_ROOT=/usr/local/android-ndk
export ANDROID_HOME=$ANDROID_SDK_ROOT
export NDK_ROOT=$ANDROID_NDK_ROOT
export GRADLE_HOME=/Users/$USER/gradle
export PATH=/usr/local/bin:$PATH:$ANDROID_NDK_ROOT
export PATH=$PATH:$ANDROID_SDK_ROOT/tools:$ANDROID_SDK_ROOT/platform-tools:$ANDROID_SDK_ROOT/build-tools/android-4.4W
